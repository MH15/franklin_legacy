<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Franklin</title>

	<link href="https://fonts.googleapis.com/css?family=Work+Sans" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="normalize.css">
	<link rel="stylesheet" type="text/css" href="main.css">
</head>
<body>

<!-- non-editable zone -->
<header>
	<h1>Franklin</h1>
</header>

<!-- editable zone -->
<h3>editable zone 1</h3>
<section franklin-state="editable-zone">
	<!-- insert all items from database here -->
	<ul>
		<% for(var i=0; i< ejsData.length; i++) {%>
			<li franklin-state="editable" class="content">
				<span><%= ejsData[i].content %></span>
			</li>
		<% } %>
	</ul>
</section>		

<!-- other editable zone, unique franklinIDs -->



<script src="franklin-lib.js"></script>
<script src="main.js"></script>


</body>
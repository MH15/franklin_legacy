 <!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title><%= meta.title %></title>

	<link href="https://fonts.googleapis.com/css?family=Work+Sans" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="/normalize.css">
	<link rel="stylesheet" type="text/css" href="/css/style.min.css">
	<link rel="shortcut icon" href="/favicon.ico">


</head>
<body>

<!-- non-editable zone -->
<header class="top">
	<h1><%= meta.title %></h1>
	<h3>second template</h3>
</header>

<!-- <p class="message"><%= meta.message %></p> -->

<!-- generate zones -->
<div class="wrapper">
	<% for(var l = 0; l < data.length; l++) {%>

		<div class="section">
			<div class="editable">
				<h2 class="title"><%= data[l].title %></h2>
				<div class="content"><%- data[l].content %></div>


				<% if (data[l].content == null) { %>
					<%- include('../templates/initSection.ejs') %>
				<% } %>
			</div>

		</div>
	<% } %>


	<!-- zone add template -->
	<%-
	include('../templates/newZone.ejs')
	%>

</div>



<%-
include('../templates/footer.ejs')

%>
<%-
include(`../templates/${meta.editorTemplate}.ejs`)
%>

<script src="https://cdn.quilljs.com/1.3.1/quill.js"></script>
<script src="/franklin-lib.js"></script>
<script src="/<%=meta.phaseScript%>"></script>


</body>